{
  "name": "multiplayer-tetris",
  "version": "1.0.0",
  "description": "Multiplayer tetris built with the MERN stack",
  "scripts": {
    "start": "nodemon ./bin/www",
    "devserver": "nodemon ./bin/www",
    "devclient": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn devserver\" \"yarn devclient\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": "https://github.com/Dereje1/Multiplayer-Tetris.git",
  "author": "Dereje Getahun",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "path": "^0.12.7",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "9.x",
    "concurrently": "^4.1.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3"
  }
}
